<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ToDoItemController">
   <children>
      <AnchorPane layoutX="-1.0" prefHeight="321.0" prefWidth="400.0">
         <children>
            <VBox prefHeight="321.0" prefWidth="228.0" spacing="10.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <Label text="Add an Item Below" />
                  <TextField fx:id="addItemTextField" maxWidth="88.0" prefHeight="25.0" prefWidth="88.0" promptText="Enter an Item" />
                  <Label text="Select a Date" />
                  <DatePicker />
                  <AnchorPane maxHeight="176.0" maxWidth="115.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="176.0" prefWidth="115.0">
                     <children>
                        <Button fx:id="exitToDoWindow" layoutY="150.0" mnemonicParsing="false" onAction="#createItem" prefHeight="25.0" prefWidth="115.0" text="Exit To Do Menu" />
                     </children>
                  </AnchorPane>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
